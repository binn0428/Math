{
  "manifest_version": 3,
  "name": "反覆練習題目生成器",
  "version": "1.0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "login.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}